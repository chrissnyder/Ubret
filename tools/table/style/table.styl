.table
  overflow: auto

  table
    font-size: 12px
    border: 1px solid #525252

    tr.selected
      background-color: #09fbdd 

    tr:nth-child(even)
      background: #f3f3f3

      &.selected
        background-color: #09fbdd 

    td, th
      padding: 5px
      border 1px solid #525252

    td
      overflow: hidden
      width: 105px
      white-space: nowrap

