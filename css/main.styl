#ubret
  width: 100%

  .dashboard
    width: 80%
    margin-bottom: 140px

    .tools
      padding: 10px

      .tool
        padding: 10px

        .bind-select
          z-index: 999

  .toolbox
    border-top: 1px $black solid
    background-color: $white
    width: 100%
    height: 140px
    margin: 0
    position: fixed
    bottom: 0
    left: 0

    .tool
      width: 80px
      padding: 10px
      margin: 10px
      float: left
