<% if @subject: %>
  <% if @subject.image: %>
    <img src="<%- @subject.image %>" />
  <% end %>

  <ul>
    <li>id: <%- @subject.zooniverse_id %></li>
    <% for key, value of @keys: %>
    <li><%- key %>: <%- @format(@subject[value]) %></li>
    <% end %>
  </ul>
<% end %>

<% if @count > 1: %>
  <a class="next">next</a>
  <a class="back">back</a>
<% end %>
