<table>
  <thead>
    <tr>
      <% if @keys.length: %>
      <% for key in @keys: %>
        <th><%= @prettyKey(key) %></th>
      <% end %>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% if @data.length: %>
    <% for subject in @data: %>
      <%- require('views/table_row')({subject, @keys}) %>
    <% end %>
    <% end %>
  </tbody>
</table>